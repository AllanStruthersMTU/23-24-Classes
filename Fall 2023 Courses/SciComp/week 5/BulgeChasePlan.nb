      RowBox[{
       RowBox[{"-", "1"}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"b", ",", "b"}], "}"}]}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"HB", "\[LeftDoubleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}], ",", 
     RowBox[{
      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}]}], 
    "\[RightDoubleBracket]"}], "=", "L"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{"Q", ",", "T"}], "}"}], "=", 
   RowBox[{"SchurDecomposition", "[", "L", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"BigQ", "=", 
   RowBox[{"IdentityMatrix", "[", "n", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"BigQ", "\[LeftDoubleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}], ",", 
     RowBox[{
      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}]}], 
    "\[RightDoubleBracket]"}], "=", "Q"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HS", "=", 
