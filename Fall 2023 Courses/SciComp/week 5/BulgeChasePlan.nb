      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}]}], 
    "\[RightDoubleBracket]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HBp", "=", "HB"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"HBp", "\[LeftDoubleBracket]", 
    RowBox[{
     RowBox[{
      RowBox[{"n", "-", "b", "+", "1"}], ";;", "n"}], ",", 
