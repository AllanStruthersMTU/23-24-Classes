02ce0ea63338"],

Cell[TextData[{
 "Definition: The ",
 StyleBox["angle between",
  FontVariations->{"Underline"->True}],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", ",", 
    RowBox[{"v", "\[Element]", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4e7ff676-bb47-43b8-a677-677bc3ef6d96"],
 " is defined by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", ".", "v"}], "=", 
    RowBox[{"||", "u", "||", "||", "v", "||", 
     RowBox[{"cos", "(", "\[Theta]", ")"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fda3ca5c-1e28-4fbf-a91a-fbbcdcf48c0a"],
 ". They are ",
 StyleBox["orthogonal",
  FontVariations->{"Underline"->True}],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", ".", "v"}], "=", "0"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "da60ad7d-48cb-41f3-8839-967a59b40440"],
 "."
}], "Text",
