 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["W", "\[Perpendicular]"], "=", 
    RowBox[{"{", 
     RowBox[{"z", ":", " "}]}]}], TraditionalForm]],ExpressionUUID->
