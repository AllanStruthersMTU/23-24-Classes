Cell[TextData[{
 "We have a simple recipe for the coefficients ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["c", "i"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "70a1893f-bae9-41e8-a59f-3e76a0bb6718"],
 ".  It could be simpler if we normalized the basis to have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "i"], ".", 
     SubscriptBox["u", "i"]}], "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2a45d2b8-1530-4135-84e3-ffaebee50808"],
 "."
}], "Text",
