Cell[TextData[{
 "Reminder Definition: The ",
 StyleBox["row space ",
  FontVariations->{"Underline"->True}],
 "of a matrix ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", 
     RowBox[{"m", "\[Times]", "n"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3ab682f3-fcd6-49b6-a343-e71da23ea26a"],
