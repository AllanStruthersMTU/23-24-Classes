  0.88, 1, 0.88],ExpressionUUID->"780066e7-5c6f-4da1-9594-b182bce078ec"],

Cell[TextData[{
 "Theorem 2: Pythagorean\nVectors ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", ",", 
    RowBox[{"v", "\[Element]", 
     SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1e343abd-27ca-46f8-9f13-a572f525d8f6"],
 " are orthogonal iff ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"||", 
     RowBox[{"u", "+", "v"}], 
     SuperscriptBox["||", "2"]}], "=", 
    RowBox[{"||", "u", 
     SuperscriptBox["||", "2"], 
     RowBox[{"+", 
      RowBox[{"||", "v", 
       SuperscriptBox["||", "2"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1eb7842c-c128-4911-bdeb-af1e29bcf0d7"],
 " "
