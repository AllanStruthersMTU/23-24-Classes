Cell[TextData[{
 "The product AB is defined if the number of columns of A matches the number \
of rows of B. \nThe matrix of ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", " ", "B"}], TraditionalForm]],ExpressionUUID->
  "63c44f03-7ee2-4664-8b53-314ddf0e1fc8"],
 " is  ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     RowBox[{"A", " ", 
      SubscriptBox["b", "1"]}], " ", "|", " ", 
     RowBox[{"A", " ", 
      SubscriptBox["b", "2"]}], " ", "|", " ", "\[Ellipsis]", " ", "|", " ", 
     RowBox[{"A", " ", 
      SubscriptBox["b", "p"]}]}], "]"}], TraditionalForm]],ExpressionUUID->
  "853e4eb6-a0a9-40fe-8700-8e583f914f61"]
}], "Text",
 CellChangeTimes->{{3.9037703190921535`*^9, 3.903770339531974*^9}, {
  3.9037709214609504`*^9, 3.903771168227668*^9}, {3.904189600175859*^9, 
  3.9041898142740383`*^9}, {3.904189851941903*^9, 3.904190100228673*^9}, {
  3.9041901560266523`*^9, 3.9041902204854717`*^9}, {3.904190538410961*^9, 
  3.904190549267721*^9}, {3.9042038503769407`*^9, 
  3.9042038505421333`*^9}},ExpressionUUID->"8e2841d3-f731-4f79-97fa-\
6db70c5bd6ab"],

Cell[TextData[Cell[BoxData[
 FormBox[
  FrameBox[
   RowBox[{
    RowBox[{
     RowBox[{"If", " ", "A"}], " ", "\[Element]", " ", 
     RowBox[{
      SuperscriptBox["\[DoubleStruckCapitalR]", 
       RowBox[{"m", "\[Times]", "n"}]], " ", "and", " ", "B"}], " ", 
     "\[Element]", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DoubleStruckCapitalR]", 
        RowBox[{"n", "\[Times]", "p"}]], "then", " ", "for", " ", "1"}], " ", 
      "<=", " ", "i", " ", "<=", " ", 
      RowBox[{"m", " ", "and", " ", "1"}], " ", "<=", " ", "j", " ", "<=", 
      " ", "p"}]}], "\n", "\t", 
    RowBox[{
     SubscriptBox["AB", 
      RowBox[{"i", ",", "j"}]], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "1"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"1", ",", "j"}]]}], "+", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "2"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"2", ",", "j"}]]}], "+", "\[Ellipsis]", "+", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "n"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"n", ",", "j"}]]}]}], "=", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "n"], 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "k"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"k", ",", "j"}]]}]}]}]}]}]], 
  TraditionalForm]],ExpressionUUID->"0ee0184a-9f7f-4a75-9b9e-7b41e05e4b6b"]], \
"Text",
 CellChangeTimes->{{3.9040279523347826`*^9, 3.9040282231361666`*^9}, {
  3.90402831291987*^9, 3.90402831291987*^9}, {3.9040283601112814`*^9, 
  3.9040283601112814`*^9}, {3.904033439530529*^9, 3.9040334620246735`*^9}, {
