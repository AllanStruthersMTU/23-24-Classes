Cell[TextData[{
 "There is a simple recipe for the coefficients ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["c", "1"], 
      SubscriptBox["u", "1"]}], "+", 
     RowBox[{
      SubscriptBox["c", "2"], 
      SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      SubscriptBox["c", "p"], 
      SubscriptBox["u", "p"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0eb94a4f-a85f-422e-a949-c259c15cf0b6"],
