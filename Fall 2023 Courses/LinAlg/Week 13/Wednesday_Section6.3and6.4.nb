Cell[TextData[{
 "Theorem 8: For an orthogonal basis ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["u", "1"], ",", 
     SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["u", "p"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "37fcd043-5786-48e9-b4ab-fce06680a37b"],
 " spanning ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"0a4c1c69-6ebd-492e-a5ae-07b1ba5418b8"],
 "and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"3984f559-1eb9-4ace-bc79-0e959475ccdd"],
 " the unique decomposition\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     OverscriptBox["y", "^"], "+", "z"}]}], TraditionalForm]],ExpressionUUID->
  "c6b24b04-8cd3-4154-a3d6-4badb6ed9c6f"],
 "\nwith ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["y", "^"], "\[Element]", "W"}], TraditionalForm]],
  ExpressionUUID->"723b791b-c8e3-4469-a189-4c5937668e1e"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "\[Element]", 
    SuperscriptBox["W", "\[Perpendicular]"]}], TraditionalForm]],
  ExpressionUUID->"490dff26-ed02-448f-b3c9-1b0d114d1966"],
 " is given by \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["y", "^"], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"y", ".", 
        SubscriptBox["u", "1"]}], 
       RowBox[{
        SubscriptBox["u", "1"], ".", 
        SubscriptBox["u", "1"]}]], 
      SubscriptBox["u", "1"]}], "+", 
     RowBox[{
      FractionBox[
       RowBox[{"y", ".", 
        SubscriptBox["u", "2"]}], 
       RowBox[{
        SubscriptBox["u", "2"], ".", 
        SubscriptBox["u", "2"]}]], 
      SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      FractionBox[
       RowBox[{"y", ".", 
        SubscriptBox["u", "p"]}], 
       RowBox[{
        SubscriptBox["u", "p"], ".", 
        SubscriptBox["u", "p"]}]], 
      SubscriptBox["u", "p"]}]}]}], TraditionalForm]],ExpressionUUID->
  "df70d94e-8cc4-4036-8a16-97ac1a1b64e9"],
 "\nand ",
 Cell[BoxData[
  FormBox[
   RowBox[{"z", "=", 
    RowBox[{"y", "-", 
     OverscriptBox["y", "^"]}]}], TraditionalForm]],ExpressionUUID->
  "f6a749b6-3989-42b3-88aa-19a8b286d4c8"],
 ". "
