Cell[TextData[{
 "In calculus you learned how to split a vector ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", " "}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f1196b15-eaa9-4fe8-8ed8-0ea7f0f5866b"],
 "into a piece parallel and perpendicular to another vector ",
 Cell[BoxData[
  FormBox["u", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e03398c2-c11b-4489-861b-9609886ab250"],
 ". \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "par"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"(", 
       RowBox[{"y", ".", "u"}], ")"}], 
      RowBox[{"u", ".", "u"}]], " ", "u"}]}], TraditionalForm]],
  ExpressionUUID->"65879fb5-3cc7-4525-bffd-d5d2c250a720"],
 "\nand \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["y", "perp"], "=", 
    RowBox[{"y", "-", 
     RowBox[{
      FractionBox[
       RowBox[{"(", 
        RowBox[{"y", ".", "u"}], ")"}], 
       RowBox[{"u", ".", "u"}]], " ", "u"}]}]}], TraditionalForm]],
  ExpressionUUID->"631d48f4-ac25-442b-bc16-6a39b219db96"]
}], "Text",
