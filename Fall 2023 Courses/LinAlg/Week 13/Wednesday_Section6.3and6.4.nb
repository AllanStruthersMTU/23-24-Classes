 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"c", " ", "u"}], ")"}], ".", "v"}], "=", 
    RowBox[{
     RowBox[{"c", " ", 
      RowBox[{"u", ".", "v"}]}], " ", "=", " ", 
     RowBox[{"u", ".", 
      RowBox[{"(", 
       RowBox[{"c", " ", "v"}], ")"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d7cb94d9-72cb-4d3c-9b5a-d6215fbdbd7e"],
 "; ",
 Cell[BoxData[
  FormBox[
