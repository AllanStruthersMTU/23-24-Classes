Cell[TextData[{
 "Theorem 5: If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["u", "p"]}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c49d0c0c-fe65-45d4-b1e8-2881fe1902ad"],
 " is an orthogonal basis for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a3b38075-3bcd-4629-b922-cf9b7b5b495e"],
 " then for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", "W"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "068b3c49-f7b6-4505-b8b1-67d22e16dedd"],
 " is the linear combination\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["c", "1"], 
       SubscriptBox["u", "1"]}], "+", 
      RowBox[{
       SubscriptBox["c", "2"], 
       SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
      RowBox[{
       SubscriptBox["c", "p"], 
       SubscriptBox["u", "p"]}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "n"], 
      RowBox[{
       SubscriptBox["c", "i"], 
       SubscriptBox["u", "i"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d69811d8-48ae-46b9-9763-c7c39a931565"],
 "\nwhere \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    FractionBox[
     RowBox[{"y", ".", 
      SubscriptBox["u", "i"]}], 
     RowBox[{
      SubscriptBox["u", "i"], ".", 
      SubscriptBox["u", "i"]}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cfc89ae3-3614-432f-bd06-46da59509e36"],
 "."
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
