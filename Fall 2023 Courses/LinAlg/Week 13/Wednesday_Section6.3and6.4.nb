Cell[TextData[{
 "There is a simple recipe for the coefficients ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["c", "1"], 
      SubscriptBox["u", "1"]}], "+", 
     RowBox[{
      SubscriptBox["c", "2"], 
      SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      SubscriptBox["c", "p"], 
      SubscriptBox["u", "p"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8c4f18a6-0fba-4b28-a3b3-a4863d2be188"],
