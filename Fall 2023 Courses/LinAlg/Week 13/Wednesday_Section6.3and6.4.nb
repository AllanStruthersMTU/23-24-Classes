Cell[TextData[{
 "Theorem 5: If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["u", "p"]}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2f403338-6094-4bf4-9be1-6b1ff145a9e5"],
