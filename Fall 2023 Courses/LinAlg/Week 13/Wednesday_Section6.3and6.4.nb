Cell[TextData[{
 "Reminder Definition: The ",
 StyleBox["row space ",
  FontVariations->{"Underline"->True}],
 "of a matrix ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", 
     RowBox[{"m", "\[Times]", "n"}]]}], TraditionalForm]],ExpressionUUID->
  "f86a1d2b-f406-4b24-adfe-2b283052ae79"],
 " is the span of the rows of ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],ExpressionUUID->
  "6ded07f4-ee30-4275-9770-c67147d9008d"],
 " and the ",
 StyleBox["column space ",
  FontVariations->{"Underline"->True}],
 "is the span of the columns. The ",
 StyleBox["null space",
  FontVariations->{"Underline"->True}],
 " of A is the set of all solutions of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "x"}], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "7aa4eee3-7744-49af-b25c-788b2eb7183b"],
 ".  Clearly\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"row", "(", "A", ")"}], "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"5c6100cd-4c03-429a-a205-c62c95b24ecd"],
 ",  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"col", "(", "A", ")"}], "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "m"]}], TraditionalForm]],
  ExpressionUUID->"42be4bfd-7743-43e7-ba29-dae7c3d4335d"],
 ", ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"nul", "(", "A", ")"}], "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"35f99a34-fe8f-40d9-88f9-49b56839efa6"],
 ", and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"nul", "(", 
     RowBox[{"A", "\[Transpose]"}], ")"}], "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "m"]}], TraditionalForm]],
  ExpressionUUID->"36e90075-8d8c-4e75-92c5-924da7e49a5c"]
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
   3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
   3.909484424879016*^9}, {3.909484613582157*^9, 3.909484817332931*^9}, 
   3.909484862832632*^9, {3.909485061004411*^9, 3.909485221317504*^9}, {
