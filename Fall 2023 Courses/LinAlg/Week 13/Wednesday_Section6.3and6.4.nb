Cell[TextData[{
 "Definition: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    OverscriptBox["y", "^"], "=", 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "p"], 
     RowBox[{
      FractionBox[
       RowBox[{"y", ".", 
        SubscriptBox["u", "i"]}], 
       RowBox[{
        SubscriptBox["u", "i"], ".", 
        SubscriptBox["u", "i"]}]], 
      SubscriptBox["u", "i"]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b430bd10-c36a-4bdb-b9b0-23b3c9e59a72"],
 " is the orthogonal projection of ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "1bcced07-34c6-4b51-abc2-0cd940f7d34f"],
 " onto the span of the orthogonal basis ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["u", "1"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["u", "p"]}], "}"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ea189f1b-33b7-443c-a7d4-34cb4128c242"],
 " "
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
