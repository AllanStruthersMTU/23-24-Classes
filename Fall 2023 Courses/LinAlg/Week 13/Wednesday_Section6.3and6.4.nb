Cell[TextData[{
 "Theorem 9: Best Approximation\nFor any subspace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "18fecc3f-ddb1-4141-9beb-7ef79099eed9"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"3969f9c4-b0e9-4265-8286-f21ae67e333c"],
 " spanning ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "44922a26-8795-48fb-b02d-145a4949d1e7"],
 "and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0677a7d2-c706-46d6-8814-9b6701bc431d"],
 " the unique decomposition\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"||", 
    RowBox[{"y", "-", 
     OverscriptBox["y", "^"]}], "||", 
    RowBox[{"<", " ", 
     RowBox[{"||", 
      RowBox[{
       RowBox[{"y", "-", "v"}], "||"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "329a5d22-e95a-49fa-86c9-fc60694cc431"],
 " \nfor all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"v", "\[NotEqual]", 
    OverscriptBox["y", "^"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "66d6ff15-0195-4304-a939-6a71921c9e69"],
 "."
}], "Text",
