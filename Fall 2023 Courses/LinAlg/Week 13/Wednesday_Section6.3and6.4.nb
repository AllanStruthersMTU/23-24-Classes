Cell[CellGroupData[{

Cell["6.2 Orthogonal Sets", "Subsection",
 CellChangeTimes->{{3.9022121076966515`*^9, 3.9022121582154217`*^9}, {
  3.902753149564061*^9, 3.902753160454166*^9}, {3.903257170993367*^9, 
  3.9032571816654167`*^9}, {3.9033481088363304`*^9, 3.9033481177150135`*^9}, {
  3.903769157829028*^9, 3.903769170909472*^9}, {3.909484073129572*^9, 
  3.909484084627141*^9}, {3.909485796050621*^9, 
  3.909485816190303*^9}},ExpressionUUID->"80e1b743-cf9e-46e7-bd91-\
d2f1b3acba74"],

Cell["The motto is orthogonal sets are very useful. ", "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909486072051037*^9, 
  3.9094860831173477`*^9}},ExpressionUUID->"aa090d75-479c-49ae-8bed-\
438c5a590c56"],

Cell[TextData[{
 "Definition: A set of vectors For ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["u", "p"]}], "}"}], "in", "  ", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"e6c05726-b916-4343-afcf-da2e5030ee75"],
 " is ",
 StyleBox["orthogonal",
  FontVariations->{"Underline"->True}],
 " if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "i"], ".", 
     SubscriptBox["u", "j"]}], "=", "0"}], TraditionalForm]],ExpressionUUID->
  "5da4b50d-86ec-4a7e-94ff-4e38d47512ab"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"i", "\[NotEqual]", "j"}], TraditionalForm]],ExpressionUUID->
  "8e3b0e5c-ad66-44f8-8f46-6c7fac41680e"],
 "."
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.9094843056450357`*^9}, {3.9094858328641024`*^9, 3.90948590614466*^9}},
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"d1d484c7-0947-42d5-aaee-6ec90f2589f5"],

Cell[TextData[{
 "Theorem 4: If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["u", "p"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "11a8fc76-f5bd-474d-a302-ef3a96c78441"],
 " is an orthogonal set of non-zero vectors the ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "0b65aeac-c665-441b-b8c1-6e2915398879"],
 " is Linearly Independent (LI). As a result ",
 Cell[BoxData[
  FormBox["S", TraditionalForm]],ExpressionUUID->
  "1995bc53-929d-490b-a989-3c24b8ea95e9"],
 " is a basis for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"span", "(", "S", ")"}], TraditionalForm]],ExpressionUUID->
  "43d0f671-241d-41df-b2ef-bb0e32e9cf12"],
 "."
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.9094845791252003`*^9}, {3.9094859297388787`*^9, 3.909486062425968*^9}},
 Background->RGBColor[
  0.94, 0.88, 0.94],ExpressionUUID->"347c5543-26d3-4e91-a8f0-59caac91d4b2"],

Cell[TextData[{
 "Definition: An ",
 StyleBox["orthogonal basis",
  FontVariations->{"Underline"->True}],
 " for a subspace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"bbe6c194-c92f-44c2-9465-3e8f9a053107"],
 " is a basis for ",
 Cell[BoxData[
  FormBox["W", TraditionalForm]],ExpressionUUID->
  "9b4798ee-13e8-4a3c-b67b-e182d6da7d1e"],
 " that is orthogonal. "
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.909484424879016*^9}, {3.909484613582157*^9, 3.9094846700817895`*^9}, {
  3.9094860985045476`*^9, 3.9094861813843174`*^9}},
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"192743fa-0521-452e-934d-076ba9a6d34d"],

Cell[TextData[{
 "There is a simple recipe for the coefficients ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["c", "1"], 
      SubscriptBox["u", "1"]}], "+", 
     RowBox[{
      SubscriptBox["c", "2"], 
      SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      SubscriptBox["c", "p"], 
      SubscriptBox["u", "p"]}]}]}], TraditionalForm]],ExpressionUUID->
  "0ed7aa63-6738-45e1-9493-f9b14b8b32e9"],
 " if the us are orthogonal. "
}], "Text",
 CellChangeTimes->{{3.909486516645464*^9, 
  3.9094865539409485`*^9}},ExpressionUUID->"9ec5f4e8-6493-4ffd-8692-\
2658cd97b57a"],

Cell[TextData[{
 "Theorem 5: If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "=", 
    RowBox[{"{", 
     RowBox[{
      SubscriptBox["u", "1"], ",", 
      SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["u", "p"]}], "}"}]}], TraditionalForm]],ExpressionUUID->
  "4b957b6d-c1ec-4096-a5da-67e6b9a08cef"],
 " is an orthogonal basis for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"41460eb5-45f2-44da-9844-0b6ad0007025"],
 " then for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", "W"}], TraditionalForm]],ExpressionUUID->
  "ba300f09-14b3-4239-9021-741a608d87e1"],
 " is the linear combination\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["c", "1"], 
       SubscriptBox["u", "1"]}], "+", 
      RowBox[{
       SubscriptBox["c", "2"], 
       SubscriptBox["u", "2"]}], "+", "\[Ellipsis]", "+", 
      RowBox[{
       SubscriptBox["c", "p"], 
       SubscriptBox["u", "p"]}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "n"], 
      RowBox[{
       SubscriptBox["c", "i"], 
       SubscriptBox["u", "i"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "aa13b0b5-ac9c-424b-baff-f55c81831d8e"],
 "\nwhere \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    FractionBox[
     RowBox[{"y", ".", 
      SubscriptBox["u", "i"]}], 
     RowBox[{
      SubscriptBox["u", "i"], ".", 
      SubscriptBox["u", "i"]}]]}], TraditionalForm]],ExpressionUUID->
  "0a8b5b00-0c8d-41e8-8628-f5932865d056"],
 "."
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.9094845791252003`*^9}, {3.9094859297388787`*^9, 3.909486062425968*^9}, {
  3.909486194675988*^9, 3.9094863381607504`*^9}, {3.9094864142731376`*^9, 
  3.909486416832405*^9}},
 Background->RGBColor[
  0.94, 0.88, 0.94],ExpressionUUID->"e17f3c71-0da2-49cc-aeb9-c226b69da629"],

Cell[TextData[{
 "  It would be even simpler if we normalized the basis to have ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["u", "i"], ".", 
     SubscriptBox["u", "i"]}], "=", "1"}], TraditionalForm]],ExpressionUUID->
  "3d7158b7-374a-4e56-920c-395702d0bd7a"],
 "."
}], "Text",
 CellChangeTimes->{{3.9094863423794575`*^9, 3.909486432671155*^9}, 
   3.9094865118328495`*^9, {3.909486557488951*^9, 
   3.909486561582845*^9}},ExpressionUUID->"d07c9f56-d080-4989-8616-\
a8be82644175"],

Cell[TextData[{
 "Definition: An ",
 StyleBox["orthonormal basis",
  FontVariations->{"Underline"->True}],
 " for a subspace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  ExpressionUUID->"5b117fb4-d817-459a-85ce-58bd87e26066"],
 " is an orthogonal basis that is normalized. "
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.909484424879016*^9}, {3.909484613582157*^9, 3.9094846700817895`*^9}, {
  3.9094860985045476`*^9, 3.9094861813843174`*^9}, {3.909495088830662*^9, 
  3.909495127488954*^9}},
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"d9f7a42e-80be-4d85-bead-bb8f52dfb288"],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["u", "1"], ",", 
     SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["u", "p"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "37b0920f-e95a-4a4c-bd7d-6396d12b2249"],
 " is an orthogonal basis for ",
 Cell[BoxData[
  FormBox["W", TraditionalForm]],ExpressionUUID->
  "4ba4a596-e4e2-4477-b724-a836380d253c"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    FormBox[
     RowBox[{"{", 
      RowBox[{
       FractionBox[
        SubscriptBox["u", "1"], 
        RowBox[{"||", 
         SubscriptBox["u", "1"], "||"}]], ",", 
       FractionBox[
        SubscriptBox["u", "2"], 
        RowBox[{"||", 
         SubscriptBox["u", "2"], "||"}]], ",", "\[Ellipsis]", ",", 
       FractionBox[
        SubscriptBox["u", "p"], 
        RowBox[{"||", 
         SubscriptBox["u", "p"], "||"}]]}], "}"}],
     TraditionalForm], " "}], TraditionalForm]],ExpressionUUID->
  "b8aa1171-7f20-49db-93ef-7c44aedd5e11"],
 "is an orthogonal basis for ",
 Cell[BoxData[
  FormBox["W", TraditionalForm]],ExpressionUUID->
  "2ceaff1b-be75-4299-bf9a-efe6b01d4828"],
 ". "
}], "Text",
 CellChangeTimes->{{3.909495146472763*^9, 
  3.9094952735670695`*^9}},ExpressionUUID->"7328e481-6a32-4fe9-9c1d-\
c7b4daa223af"],

Cell[TextData[{
 "Theorem 6: An ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "\[Times]", "n"}], TraditionalForm]],ExpressionUUID->
  "a18b4037-8e09-461e-bdd5-f71b4adb89fd"],
 " matrix U has orthonormal columns iff ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"U", "\[Transpose]"}], " ", "U"}], "=", 
    SubscriptBox["I", "n"]}], TraditionalForm]],ExpressionUUID->
  "c85b4fc7-cc16-45d4-9c84-309113746885"],
 ". "
}], "Text",
 CellChangeTimes->{{3.909484088566098*^9, 3.9094841034633856`*^9}, {
  3.909484155520605*^9, 3.9094842663629465`*^9}, {3.9094842984731083`*^9, 
  3.9094845791252003`*^9}, {3.9094859297388787`*^9, 3.909486062425968*^9}, {
  3.909486194675988*^9, 3.9094863381607504`*^9}, {3.9094864142731376`*^9, 
  3.909486416832405*^9}, {3.9094952921751976`*^9, 3.909495376989011*^9}},
 Background->RGBColor[
  0.94, 0.88, 0.94],ExpressionUUID->"e3a4fc20-d254-4614-bda9-c81ef3425638"],

Cell[TextData[{
 "Theorem 7: If ",
 Cell[BoxData[
  FormBox[
