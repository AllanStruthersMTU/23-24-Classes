Cell[TextData[{
 "Theorem 1: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"u", ".", "v"}], "=", 
    RowBox[{"v", ".", "u"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d113685f-50d5-48c3-b44d-076495edfe94"],
 ";  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"u", "+", "v"}], ")"}], ".", "w"}], "=", 
    RowBox[{
     RowBox[{"u", ".", "w"}], "+", 
     RowBox[{"v", ".", "w"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ba531a70-a327-467d-901c-9f9d6735904a"],
