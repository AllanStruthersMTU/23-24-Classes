Cell[TextData[{
 "Theorem 10: \nIf ",
 Cell[BoxData[
  FormBox[
   RowBox[{"{", 
    RowBox[{
     SubscriptBox["u", "1"], ",", 
     SubscriptBox["u", "2"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["u", "p"]}], "}"}], TraditionalForm]],ExpressionUUID->
  "90ef01d3-2de2-47a1-8431-561a37f4e4a1"],
 " is an orthonormal basis for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"W", "\[Subset]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "705f4451-fe0f-4aa9-9b04-24a1810072d3"],
 " then\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["proj", "W"], "y"}], "=", 
    RowBox[{
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "p"], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"y", ".", 
         SubscriptBox["u", "i"]}], ")"}], 
       SubscriptBox["u", "i"]}]}], "=", 
     RowBox[{"U", " ", 
      RowBox[{"U", "\[Transpose]"}], " ", "y"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7ec14cc0-a113-4769-bd5f-4c5b7452e57c"],
 "\nfor all ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "fedbbf99-df46-43ea-941e-176da250a0ad"]
}], "Text",
