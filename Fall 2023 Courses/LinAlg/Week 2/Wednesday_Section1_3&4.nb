}, Open  ]],

Cell[TextData[{
 "Yes.  The system ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "x"}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "06ae3e89-5deb-4392-a369-79d71ea3ae35"],
 " is consistent. ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      FractionBox["14", "5"], 
      SubscriptBox["a", "1"]}], "-", 
     RowBox[{
      FractionBox["3", "5"], 
      SubscriptBox["a", "2"]}]}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c5f18a4d-067f-48d8-b4a6-427fc6866656"],
 ". "
}], "Text",
 CellChangeTimes->{{3.9027561065522623`*^9, 3.9027561461407495`*^9}, {
  3.90275619337281*^9, 
  3.9027562035172315`*^9}},ExpressionUUID->"07347911-497c-441e-8e4e-\
c67d9b2157d3"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "Example 2.  Is ",
 Cell[BoxData[
  FormBox[
