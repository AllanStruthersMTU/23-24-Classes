Cell[TextData[{
 "A l",
 StyleBox["inear combination",
  FontVariations->{"Underline"->True}],
 " of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "1"], ",", 
    SubscriptBox["v", "2"], ",", "\[Ellipsis]", ",", 
    SubscriptBox["v", "p"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "eb4deabd-f1d0-459f-a9b6-0cfe834fe145"],
 " in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "n"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "92514fe8-1915-458d-a446-9fb221404622"],
 " with scalar ",
 StyleBox["weights",
  FontVariations->{"Underline"->True}],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "1"], ",", 
    SubscriptBox["c", "2"], ",", "\[Ellipsis]", ",", 
    SubscriptBox["c", "p"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a554a7a2-7977-4c70-a6e0-f579839b381c"],
 " is the vector\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["c", "1"], 
       SubscriptBox["v", "1"]}], "+", 
      RowBox[{
       SubscriptBox["c", "2"], 
       SubscriptBox["v", "2"]}], "+", "\[Ellipsis]", "+", 
      RowBox[{
       SubscriptBox["c", "p"], 
       SubscriptBox["v", "p"]}]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "p"], 
      RowBox[{
       SubscriptBox["c", "i"], 
       SubscriptBox["v", "i"]}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5178e50d-23f3-4ea3-b03b-6b086ff5a449"],
 "\nThe ",
 StyleBox["span",
  FontVariations->{"Underline"->True}],
 " of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "1"], ",", 
    SubscriptBox["v", "2"], ",", "\[Ellipsis]", ",", 
    SubscriptBox["v", "p"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3a88cda2-cf75-4445-9010-e9b047da51ed"],
 " in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "n"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "0461f94e-d299-40eb-979b-3ed489c44070"],
 " is the set of all linear combinations. In symbols\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"span", "(", 
     RowBox[{
      SubscriptBox["v", "1"], ",", 
      SubscriptBox["v", "2"], ",", "\[Ellipsis]", ",", 
      SubscriptBox["v", "p"]}], ")"}], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{
       SubscriptBox["c", "1"], 
       SubscriptBox["v", "1"]}], "+", 
      RowBox[{
       SubscriptBox["c", "2"], 
       SubscriptBox["v", "2"]}], "+", "\[Ellipsis]", "+", 
      RowBox[{
       SubscriptBox["c", "p"], 
       SubscriptBox["v", "p"], " ", "with", " ", Cell[TextData[Cell[BoxData[
        FormBox[
         RowBox[{
          RowBox[{"-", "\[Infinity]"}], "<", 
          SubscriptBox["c", "i"], "<", "\[Infinity]"}], TraditionalForm]],
        FormatType->TraditionalForm,ExpressionUUID->
        "d35cf99a-547f-4740-b4e5-fb24e27bda07"]],ExpressionUUID->
        "63580842-5963-4245-a516-1d1aecb15da7"]}]}], "}"}]}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "08bfd59e-f3e4-438d-8e36-919729a8674b"],
 "."
}], "Text",
