Cell[TextData[{
 "We want to work out how to tell if a vector ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "n"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "02db6c53-05a5-4b32-9cea-c844d2278302"],
 " is in the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"span", "(", 
    RowBox[{
     SubscriptBox["a", "1"], ",", 
     SubscriptBox["a", "2"], ",", "\[Ellipsis]", ",", 
     SubscriptBox["a", "m"]}], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "34dbad4b-df70-4394-a9d4-195188651025"],
 ". "
}], "Text",
 CellChangeTimes->{{3.902754366885046*^9, 3.902754502261284*^9}, {
  3.9027545428106394`*^9, 3.902754618403284*^9}, {3.902754817572624*^9, 
  3.902754829728713*^9}, {3.902754901793641*^9, 3.902754941396883*^9}, {
  3.902755228732497*^9, 
  3.902755395017209*^9}},ExpressionUUID->"5c27078c-140e-4c83-82dd-\
cd4afd7b11c5"],

Cell[TextData[{
 "First lets look at it for our two vectors ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c11b85c5-dd18-4fab-ae0a-0c1c07ca5aa5"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cf59c7ed-1900-479f-a351-cbf7cad8d5e3"],
 " in ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "3"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "48dd0564-cc92-4519-977b-89c1b343664e"],
 ".  ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "087d136a-dddf-4943-afd3-6441b1664e5f"],
 " is in ",
 Cell[BoxData[
  FormBox[
   RowBox[{"span", "(", 
    RowBox[{
     SubscriptBox["a", "1"], ",", 
     SubscriptBox["a", "2"]}], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "11b80b4f-b238-4c77-9002-e9f3c67a1f16"],
 " iff there are scalars ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "dee9f64c-50ff-40cc-9f4b-779b606161c5"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["x", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "bc527606-ca43-431e-9e7d-ff643de18f48"],
 " satisfying\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["a", "1"], 
      SubscriptBox["x", "1"]}], "+", 
     RowBox[{
      SubscriptBox["a", "2"], 
      SubscriptBox["x", "2"]}]}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d1db0833-8a71-48a0-b55a-8f0d151026f9"],
 ".\nFor the example above, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", 
    RowBox[{"(", GridBox[{
       {"1"},
       {"2"},
       {"3"}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d4ca202f-0922-44a5-ac03-5fd471561df7"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "2"], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{"-", "2"}]},
       {"1"},
       {"4"}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "92625143-7a39-4123-9665-eb4b0f62e2e4"],
 "  and our equation is \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {"1"},
         {"2"},
         {"3"}
        }], ")"}], 
      SubscriptBox["x", "1"]}], "+", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{"-", "2"}]},
         {"1"},
         {"4"}
        }], ")"}], 
      SubscriptBox["x", "2"]}]}], "=", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["b", "1"]},
       {
        SubscriptBox["b", "2"]},
       {
        SubscriptBox["b", "3"]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6fc7184f-dda2-4dba-b248-f8dff889b0ef"],
 "  or ",
 Cell[BoxData[
  FormBox[GridBox[{
     {
      RowBox[{
       SubscriptBox["x", "1"], "-", 
       RowBox[{"2", 
        SubscriptBox["x", "2"]}]}], "=", 
      SubscriptBox["b", "1"]},
     {
      RowBox[{
       RowBox[{"2", 
        SubscriptBox["x", "1"]}], "+", " ", 
       SubscriptBox["x", "2"]}], "=", 
      SubscriptBox["b", "2"]},
     {
      RowBox[{
       RowBox[{"3", " ", 
        SubscriptBox["x", "1"]}], "+", 
       RowBox[{"4", " ", 
        SubscriptBox["x", "2"]}]}], "=", 
      SubscriptBox["b", "3"]}
    }], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "6367bb36-f322-4d1d-ae98-5960111d6192"],
 " or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "x"}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "630d5c3d-9955-4398-8824-ce625a7aadcc"],
 " \nwhere the vectors ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "1"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "b40ba371-3a6e-40f6-9d59-1c991e7e7da2"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "2"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d12d8be8-865b-450a-80b5-3c3762071f73"],
 " are the columns of the ",
 Cell[BoxData[
  FormBox[
   RowBox[{"3", "\[Times]", "2"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3e3e0603-6d41-4e44-bdcb-826e5aef992e"],
 " matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "3bf668a3-042f-4767-b204-20359eb401cf"],
 " \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{
     RowBox[{"(", GridBox[{
        {"1", 
         RowBox[{"-", "2"}]},
        {"2", "1"},
        {"3", "4"}
       }], ")"}], "=", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["a", "1"], 
         RowBox[{
          SubscriptBox["a", "2"], ")"}]}
       }]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9d9a4768-e22b-4854-a62f-bc2ec5db613a"],
 ".\nWe know how to use row-reduction to tell if the equation ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", " ", "x"}], "=", "b"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "33691a53-00f8-434b-a088-89c2fd8fea88"],
 " has a solution. "
}], "Text",
 CellChangeTimes->{{3.902754366885046*^9, 3.902754502261284*^9}, {
  3.9027545428106394`*^9, 3.902754618403284*^9}, {3.902754817572624*^9, 
  3.902754829728713*^9}, {3.902754901793641*^9, 3.902754941396883*^9}, {
  3.902755228732497*^9, 
  3.902755970777623*^9}},ExpressionUUID->"c1624f51-0934-44ff-ac8a-\
10251cf961e2"]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "Example 1.  Is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"b", "=", 
    RowBox[{"{", 
     RowBox[{"4", ",", "5", ",", "6"}], "}"}], " "}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "81b310a4-3635-44c6-a6d4-5ef75192fc8b"],
 "in the span of ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "1"], "=", 
    RowBox[{"{", 
     RowBox[{"1", ",", "2", ",", "3"}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d0884e09-b9ea-49d3-91ea-ebf18235063a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "2"], "=", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"-", "2"}], ",", "1", ",", "4"}], "}"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "49b325ed-1552-4a61-acb1-a0895539466e"],
 "?"
}], "Subsubsection",
 CellChangeTimes->{{3.902755982263011*^9, 3.9027560275171747`*^9}, {
  3.9027561498604465`*^9, 
  3.9027561867683434`*^9}},ExpressionUUID->"5b72c836-44a7-4d2c-9117-\
d8b9fcb48f33"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Aug", "=", 
   RowBox[{"(", GridBox[{
      {"1", 
       RowBox[{"-", "2"}], "4"},
      {"2", "1", "5"},
      {"3", "4", "6"}
     }], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"RowReduce", "[", "Aug", "]"}]}], "Input",
 CellChangeTimes->{{3.902756033904815*^9, 3.9027560790827894`*^9}},
