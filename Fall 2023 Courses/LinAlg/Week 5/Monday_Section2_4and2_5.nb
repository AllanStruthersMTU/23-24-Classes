 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     RowBox[{"A", " ", 
      SubscriptBox["b", "1"]}], " ", "|", " ", 
     RowBox[{"A", " ", 
      SubscriptBox["b", "2"]}], " ", "|", " ", "\[Ellipsis]", " ", "|", " ", 
     RowBox[{"A", " ", 
      SubscriptBox["b", "p"]}]}], "]"}], TraditionalForm]],ExpressionUUID->
  "853e4eb6-a0a9-40fe-8700-8e583f914f61"]
}], "Text",
