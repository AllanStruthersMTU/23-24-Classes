Cell[TextData[Cell[BoxData[
 FormBox[
  FrameBox[
   RowBox[{
    RowBox[{
     RowBox[{"If", " ", "A"}], " ", "\[Element]", " ", 
     RowBox[{
      SuperscriptBox["\[DoubleStruckCapitalR]", 
       RowBox[{"m", "\[Times]", "n"}]], " ", "and", " ", "B"}], " ", 
     "\[Element]", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox["\[DoubleStruckCapitalR]", 
        RowBox[{"n", "\[Times]", "p"}]], "then", " ", "for", " ", "1"}], " ", 
      "<=", " ", "i", " ", "<=", " ", 
      RowBox[{"m", " ", "and", " ", "1"}], " ", "<=", " ", "j", " ", "<=", 
      " ", "p"}]}], "\n", "\t", 
    RowBox[{
     SubscriptBox["AB", 
      RowBox[{"i", ",", "j"}]], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "1"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"1", ",", "j"}]]}], "+", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "2"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"2", ",", "j"}]]}], "+", "\[Ellipsis]", "+", 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "n"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"n", ",", "j"}]]}]}], "=", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], "n"], 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "k"}]], " ", 
        SubscriptBox["b", 
         RowBox[{"k", ",", "j"}]]}]}]}]}]}]], 
  TraditionalForm]],ExpressionUUID->"0ee0184a-9f7f-4a75-9b9e-7b41e05e4b6b"]], \
"Text",
