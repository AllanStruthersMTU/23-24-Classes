      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5fd90a50-49c9-4093-a3c7-93e311d0d690"],
 " then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"A", "+", "B"}], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         SubscriptBox["A", "11"], "+", 
         SubscriptBox["B", "11"]}], 
        RowBox[{
         SubscriptBox["A", "12"], "+", 
         SubscriptBox["B", "12"]}], 
        RowBox[{
         SubscriptBox["A", "13"], "+", 
         SubscriptBox["B", "13"]}]},
       {
        RowBox[{
         SubscriptBox["A", "21"], "+", 
         SubscriptBox["B", "21"]}], 
        RowBox[{
         SubscriptBox["A", "22"], "+", 
         SubscriptBox["B", "22"]}], 
        RowBox[{
         SubscriptBox["A", "23"], "+", " ", 
         SubscriptBox["B", "23"]}]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "29ca594a-235b-42d3-85bf-eed122055a9c"]
}], "Text",
