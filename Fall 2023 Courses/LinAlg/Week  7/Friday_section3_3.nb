Cell[TextData[{
 "Theorem 2: If T is a triangular ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Times]", "n"}], TraditionalForm]],ExpressionUUID->
  "7ab7eb0a-01f6-409b-9868-e3b596c8975d"],
 " matrix then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"det", "(", "T", ")"}], TraditionalForm]],ExpressionUUID->
  "435e45cc-b05f-4aa3-8712-478e437c12de"],
 " is the product of the diagonal entries\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"det", "(", "T", ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["T", 
       RowBox[{"1", ",", "1"}]], "\[Times]", 
      SubscriptBox["T", 
       RowBox[{"2", ",", "2"}]], "\[Times]", "\[Ellipsis]", "\[Times]", 
      SubscriptBox["T", 
       RowBox[{"n", ",", "n"}]]}], "=", 
     RowBox[{
      SubsuperscriptBox["\[CapitalPi]", 
       RowBox[{"i", "=", "1"}], "n"], 
      SubscriptBox["T", 
       RowBox[{"i", ",", "i"}]]}]}]}], TraditionalForm]],ExpressionUUID->
  "f514fa1c-2a40-4948-9b51-893b3c3d1d4b"]
}], "Text",
 CellChangeTimes->{{3.905946487093292*^9, 3.9059466079309053`*^9}, {
