Cell[TextData[{
 "A matrix ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{
     RowBox[{"[", 
      RowBox[{
       SubscriptBox["a", "1"], " ", 
       SubscriptBox["a", "2"], " ", "\[Ellipsis]", " ", 
       SubscriptBox["a", "n"]}], "]"}], "\[Element]", 
     SuperscriptBox["\[DoubleStruckCapitalR]", 
      RowBox[{"m", "\[Times]", "n"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "27d99779-fef6-4df4-8841-5ccd98efecbd"],
 " is a set of ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7d04373d-4927-4019-8050-0e29c361cf69"],
 " columns of the vectors ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", "j"], "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", "m"]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "a8f80f7a-687d-4b89-90a5-4cc862bee3de"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", "j", "\[LessEqual]", "n"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2f8b1b3f-4856-45c0-b728-a954c44274e4"],
 ".   "
}], "Text",
 CellChangeTimes->{{3.903769191658572*^9, 
  3.9037694213585668`*^9}},ExpressionUUID->"aa985b15-87d9-4a0c-b98d-\
dfb0a2f168a3"],

Cell[TextData[{
 "A matrix is also a rectangular array of  numbers ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["a", 
     RowBox[{"i", ",", "j"}]], "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", 
     RowBox[{"m", "\[Times]", "n"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "2ca66906-c86c-4089-8afd-11c24d724d32"],
 " with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", "i", "\[LessEqual]", "m"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "4ce7f1da-0acb-41de-b2fc-4a06f86dda2b"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", "j", "\[LessEqual]", "n"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "d35ea802-2ecf-47ec-a553-a41a15e73c57"],
 ".    "
}], "Text",
 CellChangeTimes->{{3.903769191658572*^9, 
  3.9037694462314234`*^9}},ExpressionUUID->"025020ac-248d-4975-bf20-\
98b86621b619"],

Cell[TextData[{
 "The diagonal entries of A  ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", 
    RowBox[{"1", ",", "1"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "13411fec-c931-4e32-a0b6-c860560cbfc2"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "22"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "bfa95d00-217a-4800-9cc2-22e7cf22a29d"],
 ", \[Ellipsis] form the main diagonal of A."
}], "Text",
 CellChangeTimes->{{3.903769191658572*^9, 
  3.9037695817469597`*^9}},ExpressionUUID->"43dad30e-269a-4bdc-a9e9-\
104846cd250c"],

Cell["\<\
A diagonal matrix has all off diagonal entries equal to zero. \
\>", "Text",
 CellChangeTimes->{{3.903769191658572*^9, 
  3.9037696313242035`*^9}},ExpressionUUID->"259d0b88-1f08-4bf1-bb16-\
280000fa24f9"],

Cell[TextData[{
 "A zero matrix ",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", "\[Element]", 
    SuperscriptBox["\[DoubleStruckCapitalR]", 
     RowBox[{"m", "\[Times]", "n"}]]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ac72f3c9-5fde-4b76-b421-d6dbcf588fff"],
 " has all entries zero.  If the size is not obvious you can write ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["0", 
    RowBox[{"m", ",", "n"}]], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "77c1b92a-189b-464c-98a4-b9ecba4e132e"],
 " to make it clear."
}], "Text",
 CellChangeTimes->{{3.903769191658572*^9, 
  3.9037697229638205`*^9}},ExpressionUUID->"8ac2f2ba-ab0c-4dac-bb57-\
c076642fa3a7"],

