Cell["\<\
Two matrices are equal if they are the same size (have the same dimensions) \
and corresponding entries are equal. \
\>", "Text",
 CellChangeTimes->{{3.9037698577572203`*^9, 
  3.9037699163327065`*^9}},ExpressionUUID->"db1d90a9-55bc-4daf-adc2-\
4fe9adc39b3d"],

Cell[TextData[{
 "You add matrices by adding the corresponding entries.  \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{"A", "+", "B"}], ")"}], 
     RowBox[{"i", ",", "j"}]], "=", 
    RowBox[{
     SubscriptBox["a", 
      RowBox[{"i", ",", "j"}]], "+", 
     SubscriptBox["b", 
      RowBox[{"i", ",", "j"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "bacc5f9b-af8a-4ce6-979f-6ab7be9c3b27"],
 "\t\nAdding matrices of different sizes does not make sense! "
}], "Text",
 CellChangeTimes->{{3.9037698577572203`*^9, 
  3.903770006936586*^9}},ExpressionUUID->"5c8d649d-45bf-4114-bd96-\
2a17f1daa3b7"],

Cell[TextData[{
 "Scalar multiplication by ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "\[Element]", "\[DoubleStruckCapitalR]"}], 
   TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "c675dc94-1f8c-48cd-92c1-ddf8551b788d"],
 " is also simple\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     RowBox[{"(", 
      RowBox[{"\[Alpha]", " ", "A"}], ")"}], 
     RowBox[{"i", ",", "j"}]], "=", 
    RowBox[{"\[Alpha]", " ", 
     SubscriptBox["a", 
      RowBox[{"i", ",", "j"}]]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "e5f9be3b-7b00-4e56-8e82-b92a3ca62479"],
 "\t"
}], "Text",
 CellChangeTimes->{{3.9037698577572203`*^9, 
  3.9037700766614804`*^9}},ExpressionUUID->"4e8aaf51-3380-4515-8b23-\
366c1bf60f1a"],

Cell["Not surprisingly!", "Text",
 CellChangeTimes->{{3.9037700910052443`*^9, 3.9037700972200413`*^9}, {
  3.903770262156579*^9, 
  3.903770263453957*^9}},ExpressionUUID->"9c9da748-2a8d-458f-bdca-\
0c5ba805aaa4"],

Cell[TextData[Cell[BoxData[
 FormBox[GridBox[{
    {
     RowBox[{"A", "+", "B"}], "=", 
     RowBox[{"B", "+", "A"}], " ", 
     RowBox[{"r", "(", 
      RowBox[{"A", "+", "B"}], ")"}], "=", 
     RowBox[{
      RowBox[{"r", " ", "A"}], "+", 
      RowBox[{"r", " ", "B"}]}]},
    {
     RowBox[{
      RowBox[{"(", 
       RowBox[{"A", "+", "B"}], ")"}], "+", "C"}], "=", 
     RowBox[{"A", "+", 
      RowBox[{"(", 
       RowBox[{"B", "+", "C"}], ")"}]}], " ", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"r", "+", "s"}], ")"}], "A"}], "=", 
     RowBox[{
      RowBox[{"r", " ", "A"}], "+", 
      RowBox[{"r", " ", "B"}]}]},
    {
     RowBox[{"A", "+", "0"}], "=", "A", "    ", 
     RowBox[{"r", "(", 
      RowBox[{"s", " ", "A"}], ")"}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"r", " ", "s"}], ")"}], "A"}]}
   }], TraditionalForm]],
 FormatType->
  TraditionalForm,ExpressionUUID->"0c240ddd-de21-4cd3-a8e5-8493b33c8925"]], \
"Text",
 CellChangeTimes->{{3.9037701125947704`*^9, 3.9037702426144104`*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"88d3a99b-1f5d-4b2e-9a66-cbe2a9cb4506"],

