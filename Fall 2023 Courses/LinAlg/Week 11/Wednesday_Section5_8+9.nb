Cell[TextData[{
 "A vector x with nonnegative entries that sum to one is a ",
 StyleBox["probability",
  FontVariations->{"Underline"->True}],
 " ",
 StyleBox["vector",
  FontVariations->{"Underline"->True}],
 "."
}], "Text",
