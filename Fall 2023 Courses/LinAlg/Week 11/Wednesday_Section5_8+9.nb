Cell[TextData[{
 "Inverse Power Method ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", 
     RowBox[{"k", "+", "1"}]], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
