        RowBox[{"m", "-", "1"}]}]], 
      SubscriptBox["x", 
       RowBox[{"m", "-", "1"}]]}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", "m"], "\[DoubleLongLeftRightArrow]", 
      SubscriptBox["x", 
       RowBox[{"m", "-", "1"}]]}], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["b", "m"], 
       SubscriptBox["H", 
        RowBox[{"m", ",", 
         RowBox[{"m", "-", "1"}]}]]], "-", 
      RowBox[{
       FractionBox[
        SubscriptBox["H", 
         RowBox[{"m", ",", "m"}]], 
        SubscriptBox["H", 
         RowBox[{"m", ",", 
          RowBox[{"m", "-", "1"}]}]]], 
       SubscriptBox["x", "m"]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "9dd0b57a-e35f-4617-82d4-bddf5884070d"],
 "\nand the second last is \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["H", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", "m"}]], 
      SubscriptBox["x", "m"]}], "+", 
     RowBox[{
      SubscriptBox["H", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", 
        RowBox[{"m", "-", "1"}]}]], 
      SubscriptBox["x", 
       RowBox[{"m", "-", "1"}]]}], "+", 
     RowBox[{
      SubscriptBox["H", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", 
        RowBox[{"m", "-", "2"}]}]], 
      SubscriptBox["x", 
       RowBox[{"m", "-", "2"}]]}]}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["b", 
       RowBox[{"m", "-", "1"}]], "\[DoubleLongLeftRightArrow]", 
      SubscriptBox["x", 
       RowBox[{"m", "-", "2"}]]}], "=", 
     RowBox[{
      FractionBox[
       SubscriptBox["b", 
        RowBox[{"m", "-", "1"}]], 
       SubscriptBox["H", 
        RowBox[{
         RowBox[{"m", "-", "1"}], ",", 
         RowBox[{"m", "-", "2"}]}]]], "-", 
      RowBox[{
       FractionBox[
        SubscriptBox["H", 
         RowBox[{
          RowBox[{"m", "-", "1"}], ",", "m"}]], 
        SubscriptBox["H", 
         RowBox[{"m", ",", 
          RowBox[{"m", "-", "2"}]}]]], 
       SubscriptBox["x", "m"]}], "-", 
      RowBox[{
       FractionBox[
        SubscriptBox["H", 
         RowBox[{
          RowBox[{"m", "-", "1"}], ",", 
          RowBox[{"m", "-", "1"}]}]], 
        SubscriptBox["H", 
         RowBox[{"m", ",", 
          RowBox[{"m", "-", "2"}]}]]], 
       SubscriptBox["x", 
        RowBox[{"m", "-", "1"}]]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "8fefc3ed-231c-4b71-a441-4c86db7bf36f"],
