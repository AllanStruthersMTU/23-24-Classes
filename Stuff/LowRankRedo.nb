        RowBox[{
         RowBox[{"RandomOrth", "[", 
          RowBox[{"{", 
           RowBox[{"#", ",", "s"}], "}"}], "]"}], "&"}], ",", 
        RowBox[{"{", 
         RowBox[{"m", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"TwoSidedSample", "=", 
      RowBox[{
       RowBox[{"SL", "\[Transpose]"}], ".", "A", ".", "SR"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Sigma]", "=", 
      RowBox[{"SingularValueList", "[", 
       RowBox[{"TwoSidedSample", ",", " ", 
