   RowBox[{"A_", ",", "s_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "m", ",", "n", ",", "SL", ",", "SR", ",", "TwoSidedSample", ",", 
