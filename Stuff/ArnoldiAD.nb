        RowBox[{"(*", " ", 
         RowBox[{
         "product", " ", "rule", " ", "for", " ", "dw", " ", "update"}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{"dw", "=", 
         RowBox[{"dw", "-", 
          RowBox[{
           RowBox[{"dHNew", "\[LeftDoubleBracket]", 
