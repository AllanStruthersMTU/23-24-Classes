Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ArnoldiAD1", "[", "A_", "]"}], "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Q_", ",", "H_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"dQ_", ",", "dH_"}], "}"}]}], "}"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "m", ",", "n", ",", "HNew", ",", "dHNew", ",", "w", ",", "dw", ",", "q", 
      ",", "dq", ",", "\[Gamma]", ",", "d\[Gamma]"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"m", ",", "n"}], "}"}], "=", 
      RowBox[{"Dimensions", "[", "Q", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Duplicating", " ", "the", " ", "Initialization", " ", "for", " ", 
       "HNew"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"dHNew", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"n", "+", "1"}], ",", "n"}], "}"}]}], "]"}]}], ";", 
     RowBox[{
      RowBox[{"dHNew", "\[LeftDoubleBracket]", 
       RowBox[{
        RowBox[{"1", ";;", "n"}], ",", 
        RowBox[{"1", ";;", 
         RowBox[{"n", "-", "1"}]}]}], "\[RightDoubleBracket]"}], "=", "dH"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"HNew", "  ", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"n", "+", "1"}], ",", "n"}], "}"}]}], "]"}]}], ";", "  ", 
     RowBox[{
      RowBox[{"HNew", "\[LeftDoubleBracket]", 
       RowBox[{
        RowBox[{"1", ";;", "n"}], ",", 
        RowBox[{"1", ";;", 
         RowBox[{"n", "-", "1"}]}]}], "\[RightDoubleBracket]"}], "=", "H"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Duplicating", " ", "the", " ", "computation", " ", "for", " ", "w", 
       " ", "for", " ", "dw"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"dw", " ", "=", " ", 
      RowBox[{"A", ".", 
       RowBox[{"dQ", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "n"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"w", "   ", "=", 
      RowBox[{"A", ".", 
       RowBox[{"Q", "\[LeftDoubleBracket]", 
        RowBox[{"All", ",", "n"}], "\[RightDoubleBracket]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"duplicate", " ", "for", " ", "dq"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"dq", "=", 
         RowBox[{"dQ", "\[LeftDoubleBracket]", 
          RowBox[{"All", ",", "k"}], "\[RightDoubleBracket]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"q", "=", 
         RowBox[{"Q", "\[LeftDoubleBracket]", 
          RowBox[{"All", ",", "k"}], "\[RightDoubleBracket]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"product", " ", "rule", " ", "for", " ", "dHNew"}], " ", 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"dHNew", "\[LeftDoubleBracket]", 
          RowBox[{"k", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
         RowBox[{
          RowBox[{"dq", ".", "w"}], "+", 
          RowBox[{"q", ".", "dw"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"HNew", "\[LeftDoubleBracket]", 
          RowBox[{"k", ",", "n"}], "\[RightDoubleBracket]"}], "=", 
