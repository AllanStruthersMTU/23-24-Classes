   "source": [
    "using ILUZero, BenchmarkTools, LinearAlgebra, SparseArrays\n",
    "m=1000;\n",
    "b=rand(m)\n",
    "A = sprand(m, m, 5/m) + 10I\n",
    "B=copy(A)\n",
    "fact = @time ilu0(A)\n",
    "updated_fact= ilu0(B)\n",
    "B[1,1]=3.4\n",
    "@time ilu0!(updated_fact, B)\n",
    "@time fact\\b;"
   ]
