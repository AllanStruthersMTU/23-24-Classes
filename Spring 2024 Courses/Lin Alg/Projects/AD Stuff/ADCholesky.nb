  FormatType->TraditionalForm,ExpressionUUID->
  "5153066a-7184-4317-83d6-43f16ff7e1eb"],
 "\nso \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"dA", " ", "=", " ", 
    RowBox[{
     RowBox[{"dQ", ".", "H", ".", 
      RowBox[{"Q", "\[Transpose]"}]}], "+", 
     RowBox[{"Q", ".", "dH", ".", 
      RowBox[{"Q", "\[Transpose]"}]}], "+", 
     RowBox[{"Q", ".", "H", ".", 
      RowBox[{"dQ", "\[Transpose]"}]}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "9d2773dd-5184-41bc-a76b-e1a146117032"],
 "\nso\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"Q", "\[Transpose]"}], ".", "dA", ".", "Q"}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{"Q", "\[Transpose]"}], ".", "dQ", ".", "H"}], "+", "dH", "+", 
     RowBox[{"H", ".", 
      RowBox[{"dQ", "\[Transpose]"}], ".", "Q"}]}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "8639b199-0cbb-4095-b2e7-84dec267d458"],
 "\nso with ",
 Cell[BoxData[
  FormBox[
   RowBox[{"X", "=", 
    RowBox[{
     RowBox[{"Q", "\[Transpose]"}], ".", "dQ"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "5528fa5b-43c9-4b50-884b-6a7ab6189467"],
 "  with ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"X", "\[Transpose]"}], "=", 
    RowBox[{"-", "X"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f9c919c7-6ae5-4b6f-9188-ba7fd0ee976a"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     SubscriptBox["H", "sym"], "+", 
