     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["H", "sym"], "+", 
         SubscriptBox["H", "skw"]}], ")"}], "\[Transpose]"}], ".", "X"}], "+", 
     RowBox[{"dH", "\[Transpose]"}], "+", 
     RowBox[{"X", ".", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["H", "sym"], "+", 
         SubscriptBox["H", "skw"]}], ")"}], "\[Transpose]"}]}]}]}], 
