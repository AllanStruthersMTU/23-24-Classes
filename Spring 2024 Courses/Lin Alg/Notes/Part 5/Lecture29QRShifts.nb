  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Create", " ", "the", " ", "bulge"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Compute", " ", "the", " ", "Shifts", " ", "and", " ", "the", " ", "first",
     " ", "colun", " ", "of", " ", "M"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HBot", "=", 
   RowBox[{"H", "\[LeftDoubleBracket]", 
