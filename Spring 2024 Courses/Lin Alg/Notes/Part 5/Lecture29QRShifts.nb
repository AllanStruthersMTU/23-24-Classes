Cell[TextData[{
 "\[OpenCurlyDoubleQuote]Every eigenvalue of A lies in at least one of the ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],ExpressionUUID->
  "2ce79609-872b-4277-aafb-cd7bf0be416f"],
 " circular disks with centers ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["c", "i"], "=", 
    RowBox[{"A", "\[LeftDoubleBracket]", 
     RowBox[{"i", ",", "i"}], "\[RightDoubleBracket]"}]}], TraditionalForm]],
  ExpressionUUID->"8b830f20-b67d-49fe-8fca-63988ae2cd52"],
 " and radius ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["r", "i"], "=", 
    RowBox[{
     SubscriptBox["\[Sum]", 
      RowBox[{"j", "\[NotEqual]", "i"}]], 
     RowBox[{"|", 
      RowBox[{"A", "\[LeftDoubleBracket]", 
       RowBox[{"i", ",", "j"}], "\[RightDoubleBracket]"}], "|"}]}]}], 
   TraditionalForm]],ExpressionUUID->"8de20808-97b1-4829-a04f-b6236c4c86be"],
 ". If ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],ExpressionUUID->
  "81cf368d-8548-442b-a1f7-b2961149252f"],
 " of the disks form a connected domain ",
 Cell[BoxData[
  FormBox["\[CapitalOmega]", TraditionalForm]],ExpressionUUID->
  "ed028d41-9058-4486-b044-f24762d5e7e3"],
 " disjoint from the other disks there are ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]],ExpressionUUID->
  "3b914fce-96b4-434a-9fa6-76eec96091d9"],
 " eigenvalues in ",
 Cell[BoxData[
  FormBox["\[CapitalOmega]", TraditionalForm]],ExpressionUUID->
  "940280f5-664f-403d-b8c0-59cb5b4c8b58"],
 ".\[CloseCurlyDoubleQuote]"
}], "Text",
