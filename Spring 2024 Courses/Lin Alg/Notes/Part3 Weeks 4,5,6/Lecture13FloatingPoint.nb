Cell[TextData[{
 "The Bynars https://en.wikipedia.org/wiki/11001001 count in base 2. They \
only have two symbols\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{"0", ",", "1"}], TraditionalForm]],ExpressionUUID->
  "4cb07b46-addb-43b1-882d-fecb966026cc"],
 "\nThe Star Trek episode title is \n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["11001001", "2"], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"1", "*", 
       SuperscriptBox["2", "7"]}], "+", 
      RowBox[{"1", "*", 
       SuperscriptBox["2", "6"]}], "+", 
      RowBox[{"0", "*", 
       SuperscriptBox["2", "5"]}], "+", 
