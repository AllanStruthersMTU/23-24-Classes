Cell[TextData[{
 "There are ",
 Cell[BoxData[
  FormBox[
   RowBox[{"30", "=", 
    RowBox[{
     SuperscriptBox["2", "5"], "-", "2"}]}], TraditionalForm]],ExpressionUUID->
  "a2c02643-e7e6-458b-a7dc-d4f41f41929d"],
 " remaining bit patterns they are interpreted as \n\t",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["2", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "15"}], " ", "+", "  ", 
      SubscriptBox[GridBox[{
         {
          SubscriptBox["e", "1"], 
          SubscriptBox["e", "2"], 
          SubscriptBox["e", "3"], 
          SubscriptBox["e", "4"], 
          SubscriptBox["e", "5"]}
        },
        GridBoxDividers->{"Columns" -> {{True}}, "Rows" -> {{True}}}], "2"]}],
      ")"}]], TraditionalForm]],ExpressionUUID->
  "23e11b49-7e7f-4157-ada7-7bc83c90614b"],
 "\nThe offset 15 is chosen to split the difference between big and small.  \
The exponent range is ",
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"-", "15"}], " ", "+", "  ", 
     SubscriptBox[GridBox[{
        {"0", "0", "0", "0", "1"}
       },
       GridBoxDividers->{"Columns" -> {{True}}, "Rows" -> {{True}}}], "2"]}], 
    "=", 
    RowBox[{
     FrameBox[
      RowBox[{
       RowBox[{"-", "14"}], "\[LessEqual]", "p", "\[LessEqual]", " ", "15"}]],
      "=", " ", 
     RowBox[{
      RowBox[{"-", "15"}], " ", "+", "  ", 
      SubscriptBox[GridBox[{
         {"0", "1", "1", "1", "1"}
        },
        GridBoxDividers->{"Columns" -> {{True}}, "Rows" -> {{True}}}], 
       "2"]}]}]}], TraditionalForm]],ExpressionUUID->
  "5771369b-a2a7-4af2-8c63-74d5ad19a6ff"]
}], "Subitem",
