    RowBox[{"(", GridBox[{
       {
        SubscriptBox["b", "1"]},
       {
        SubscriptBox["b", "2"]}
      }], ")"}]}], TraditionalForm]],ExpressionUUID->
  "043ecb76-50dd-4025-82a8-7d35c836385e"]
}], "Text",
