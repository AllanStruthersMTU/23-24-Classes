Cell[TextData[{
 "Equation 3: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["L", "33"], 
     SubscriptBox["x", "3"]}], "=", 
    RowBox[{
     RowBox[{
