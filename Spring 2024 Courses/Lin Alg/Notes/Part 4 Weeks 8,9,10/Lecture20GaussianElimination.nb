  RowBox[{
   RowBox[{"MyLUDecomposition", "[", "A_", "]"}], ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"m", "=", 
        RowBox[{"Length", "[", "A", "]"}]}], ",", "L", ",", 
