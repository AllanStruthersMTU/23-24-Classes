     RowBox[{"(*", " ", 
      RowBox[{"Start", " ", "Script"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"L", "=", 
       RowBox[{"IdentityMatrix", "[", "m", "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"L", "\[LeftDoubleBracket]", 
             RowBox[{"j", ",", "k"}], "\[RightDoubleBracket]"}], "=", 
