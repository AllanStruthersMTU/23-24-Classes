            RowBox[{"k", "+", "1"}], ",", "m"}], "}"}]}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", 
          RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}], ";", 
